          },
          {
            "label": "Minimized",
            "value": "Minimized"
          },
          {
            "label": "Maximized",
            "value": "Maximized"
          }
        ]
      },
      {
        "label": "Suppress Errors",
        "description": "Show or hide startup errors",
        "parameterControlType": "CheckBox",
        "parameterValueType": "Bool",
        "enumId": 8,
        "sortOrderIdx": 6,
        "isRequired": false,
        "values": [
          {
            "value": "false",
            "isDefault": true
          }
        ]
      }
    ]
