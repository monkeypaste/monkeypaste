{
  "title": "Image Annotator",
  "description": "A local, lightweight object detector trained on 80 different types of things.",
  "version": "1.0",
  "author": "Thomas Kefauver",
  "guid": "a54545be-e64e-4f51-a2fe-9206d5c64496",
  "projectUrl": "https://github.com/monkeypaste/ImageAnnotator",
