  "tags": "Image Detection, Annotation, Image, AI, Text",
  "iconUri": "https://github.com/monkeypaste/ImageAnnotator/raw/main/icon.png",
  "analyzer": {
    "inputType": {
      "image": true
    },
    "outputType": {
      "imageToken": true
    },
    "parameters": [
      {
        "label": "Confidence",
        "description": "A value between 0 and 1 to filter detected accuracy",
        "controlType": "Slider",
        "unitType": "Decimal",
        "minimum": 0,
        "maximum": 1,
        "isRequired": true,
        "paramId": "conf",
        "values": [
          {
            "value": "0.1",
            "isDefault": true
          }
        ]
      },
      {
        "label": "Source Content",
        "description": "The base64-encoded image to analyze",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isRequired": true,
        "isVisible": false,
        "paramId": "img64",
        "values": [
          {
            "value": "{ItemData}"
          }
        ]
      }
    ]
  }
