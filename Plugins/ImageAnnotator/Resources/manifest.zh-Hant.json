{
  "title": "圖像註釋器",
  "description": "完全免費的，本地和輕巧的對象檢測器對80種不同類型的對象進行了訓練。",
  "version": "1.0.2.0",
  "author": "Monkey",
  "guid": "a54545be-e64e-4f51-a2fe-9206d5c64496",
  "readmeUrl": "https://raw.githubusercontent.com/monkeypaste/ImageAnnotator/master/README.md",
  "projectUrl": "https://github.com/monkeypaste/ImageAnnotator",
  "packageUrl": "https://github.com/monkeypaste/ImageAnnotator/releases/download/v1.0.0/ImageAnnotator.zip",
  "iconUri": "https://raw.githubusercontent.com/monkeypaste/ImageAnnotator/master/icon.png",
  "packageType": "Dll",
  "tags": "圖像檢測，註釋，圖像，AI，文本",
  "analyzer": {
    "inputType": {
      "image": true
    },
    "outputType": {
      "imageToken": true
    },
    "parameters": [
      {
        "label": "信心",
        "description": "值在0到1之間以濾波檢測到的準確性",
        "controlType": "Slider",
        "unitType": "Decimal",
        "minimum": 0,
        "maximum": 1,
        "isRequired": true,
        "paramId": "conf",
        "values": [
          {
            "value": "0.1",
            "isDefault": true
          }
        ]
      },
      {
        "label": "Source Content",
        "description": "The base64-encoded image to analyze",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isRequired": true,
        "isVisible": false,
        "paramId": "img64",
        "values": [
          {
            "value": "{ClipText}"
          }
        ]
      }
    ]
  }
}