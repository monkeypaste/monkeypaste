  "iconUri": "https://raw.githubusercontent.com/monkeypaste/QrCoder/master/icon.png",
  "packageType": "Dll",
  "tags": "Core, codice QR, testo, immagine, collegamento, convertitore",
  "analyzer": {
    "inputType": {
      "text": true
    },
    "outputType": {
      "image": true
    },
    "parameters": [
      {
        "label": "Contenuto per il convertitore",
        "description": "Lo snippet da convertire",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isVisible": false,
        "paramId": 1,
        "values": [
          {
            "value": "{ClipText}"
          }
        ]
      },
      {
        "label": "Prefisso del titolo",
        "description": "Rende il titolo del codice QR in base al titolo di CLIP di origine",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isVisible": false,
        "paramId": 2,
        "values": [
          {
            "value": "{Title}"
          }
        ]
      }
    ]
  }
}