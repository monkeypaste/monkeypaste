        "paramId": 1,
        "values": [
          {
            "value": "{ClipText}"
          }
        ]
      },
      {
        "label": "Prefijo de título",
        "description": "Fabrica el título del código QR basado en el título de clips de origen",
        "controlType": "TextBox",
        "unitType": "PlainTextContentQuery",
        "isVisible": false,
        "paramId": 2,
        "values": [
          {
            "value": "{Title}"
          }
        ]
      }
    ]
