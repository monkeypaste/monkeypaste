<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        xmlns:local="using:MonkeyPaste.Avalonia">
    <Style Selector="AutoCompleteBox.common /template/ TextBox#PART_TextBox /template/ ScrollViewer#PART_ScrollViewer /template/ ScrollContentPresenter#PART_ContentPresenter">
        <Setter Property="VerticalContentAlignment"
                Value="Center" />
    </Style>
    <Style Selector="AutoCompleteBox.common">
        <Setter Property="IsTextCompletionEnabled"
                Value="False" />
        <Setter Property="MinimumPrefixLength"
                Value="0" />
      <Setter Property="MinimumPopulateDelay"
                Value="0.25" />
    </Style>
</Styles>
