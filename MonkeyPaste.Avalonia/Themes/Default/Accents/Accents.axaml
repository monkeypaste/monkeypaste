        <SolidColorBrush x:Key="ToggleSwitchStrokeOnPointerOver" Color="White" Opacity="1" />
        <SolidColorBrush x:Key="ToggleSwitchKnobFillOnPointerOver" Color="White" Opacity="1" />

        <!--  EFFECTS  -->
        <DropShadowEffect x:Key="EmptyDropShadow"
                          OffsetX="0"
                          OffsetY="0"
                          Color="{DynamicResource ThemeInteractiveColor}"
                          BlurRadius="0"
                          Opacity="0.0" />

        <DropShadowEffect x:Key="ThemeBlackDropShadow"
                          OffsetX="1"
                          OffsetY="1"
                          Color="{DynamicResource ThemeBlackColor}"
                          BlurRadius="3"
                          Opacity="0.8" />

        <DropShadowEffect x:Key="ThemeDropShadow"
                          OffsetX="1"
                          OffsetY="1"
                          Color="{DynamicResource ThemeInteractiveColor}"
                          BlurRadius="3"
                          Opacity="0.8" />
        <DropShadowEffect x:Key="ThemeGlow"
                          OffsetX="0"
                          OffsetY="0"
                          Color="{DynamicResource ThemeInteractiveColor}"
                          BlurRadius="5"
                          Opacity="0.8" />

        <DropShadowEffect x:Key="ThemeBgDropShadow"
                          OffsetX="1"
                          OffsetY="1"
                          Color="{DynamicResource ThemeInteractiveBgColor}"
                          BlurRadius="3"
                          Opacity="0.8" />

        <DropShadowEffect x:Key="ThemeBgDropBorder"
                          OffsetX="2"
                          OffsetY="2"
                          Color="{DynamicResource ThemeInteractiveBgColor}"
                          BlurRadius="0"
                          Opacity="1" />
        <DropShadowEffect x:Key="ThemeBgGlow"
                          OffsetX="0"
                          OffsetY="0"
                          Color="{DynamicResource ThemeInteractiveBgColor}"
                          BlurRadius="5"
                          Opacity="0.8" />
    </Style.Resources>
</Style>
