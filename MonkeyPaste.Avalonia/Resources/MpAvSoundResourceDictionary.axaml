<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
    <!--  BASELINE SOUNDS  -->
    <sys:String x:Key="MonkeySound">avares://MonkeyPaste.Avalonia/Assets/Sounds/Monkey1.wav</sys:String>
    <sys:String x:Key="TingSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/Ting.wav</sys:String>
    <sys:String x:Key="ChimeSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/chime.wav</sys:String>
    <sys:String x:Key="AlertSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/alert.wav</sys:String>
    <sys:String x:Key="BlipSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip.wav</sys:String>
    <sys:String x:Key="SonarSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip2.wav</sys:String>

    <!--  MINIMAL GROUP  -->
    <sys:String x:Key="MinimalCopySound">avares://MonkeyPaste.Avalonia/Assets/Sounds/Ting.wav</sys:String>
    <sys:String x:Key="MinimalPasteSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/chime.wav</sys:String>

    <sys:String x:Key="MinimalErrorSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/alert.wav</sys:String>

    <sys:String x:Key="MinimalAutoCopyOnSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip2.wav</sys:String>
    <sys:String x:Key="MinimalAutoCopyOffSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip.wav</sys:String>

    <sys:String x:Key="MinimalMousePasteOnSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip2.wav</sys:String>
    <sys:String x:Key="MinimalMousePasteOffSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip.wav</sys:String>

    <sys:String x:Key="MinimalAppendOnSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip2.wav</sys:String>
    <sys:String x:Key="MinimalAppendOffSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/blip.wav</sys:String>

    <sys:String x:Key="MinimalLoadedSound">avares://MonkeyPaste.Avalonia/Assets/Sounds/Monkey1.wav</sys:String>
</ResourceDictionary>
