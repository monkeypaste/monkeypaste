<local:MpAvUserControl xmlns="https://github.com/avaloniaui"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:local="using:MonkeyPaste.Avalonia"
                       xmlns:pcl="using:MonkeyPaste"
                       xmlns:webview="clr-namespace:WebViewControl;assembly=WebViewControl.Avalonia"
                       x:DataType="local:MpAvClipTileViewModel"
                       x:TypeArguments="local:MpAvClipTileViewModel"
                       x:CompileBindings="True"
                       mc:Ignorable="d"
                       d:DesignWidth="800"
                       d:DesignHeight="450"
                       x:Class="MonkeyPaste.Avalonia.MpAvContentWebView">
    <webview:WebView x:Name="WebView"
                     Address="{Binding EditorPath}"
                     local:MpAvCefWebViewExtension.ContentHtml="{Binding CopyItemData}"
                     local:MpAvCefWebViewExtension.IsContentReadOnly="{Binding IsContentReadOnly}"
                     HorizontalAlignment="Stretch"
                     VerticalAlignment="Stretch" />
</local:MpAvUserControl>
