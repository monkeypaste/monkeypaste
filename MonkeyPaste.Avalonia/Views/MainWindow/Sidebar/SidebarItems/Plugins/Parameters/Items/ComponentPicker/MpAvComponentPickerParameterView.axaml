<local:MpAvUserControl x:Class="MonkeyPaste.Avalonia.MpAvComponentPickerParameterView"
                       xmlns="https://github.com/avaloniaui"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:local="using:MonkeyPaste.Avalonia"
                       xmlns:pcl="using:MonkeyPaste"
                       x:TypeArguments="local:MpAvComponentPickerParameterViewModel"
                       x:DataType="local:MpAvComponentPickerParameterViewModel"
                       x:CompileBindings="True"
                       mc:Ignorable="d"
                       d:DesignHeight="450"
                       d:DesignWidth="800">
    <Border Classes="paramBorder">
        <local:MpAvPopupMenuSelector MinWidth="150"
                                     DataContext="{Binding}" />
    </Border>
</local:MpAvUserControl>
